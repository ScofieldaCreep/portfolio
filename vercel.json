{
  "rewrites": [
    {
      "source": "/blog/:path*",
      "destination": "https://chi-notion-blog.vercel.app/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://chi-notion-blog.vercel.app/blog"
    },
    {
      "source": "_next/static/:path*",
      "destination": "https://chi-notion-blog.vercel.app/_next/static/:path*"
    }
  ]
}